{% extends '/includes/layout.twig' %}

{% block head %}
<title> {{ page }} </title>
{% endblock %}



{% block content %}


{% for article in articles %}

    <p> {{ article.title }} </p>
    
    <p>  {{ article.content }} </p>

    <p>Écrit par : {{ article.created_by }} </p>
    
    <p>Posté le : {{ article.article_created_at }} </p>
    
    <p>Categorie : {{ article.category }} </p>

    <p>Tags :  {{ article.tags }} </p>

    <br></br>

  {% endfor %}
 

{% endblock %}